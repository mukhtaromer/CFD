FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      turbulenceProperties:nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-05;

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue uniform 1e-05;
        value           uniform 1e-05;
    }
    outlet
    {
        type            freestream;
        freestreamValue uniform 1e-05;
        value           uniform 1e-05;
    }
    walls
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
